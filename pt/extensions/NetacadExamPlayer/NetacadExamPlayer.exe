#!/bin/sh
appname=`basename $0 | sed s,\.exe$,,`
echo "starting $appname"

dirname=`dirname $0`
tmp="${dirname#?}"

if [ "${dirname%$tmp}" != "/" ]; then
dirname=$PWD/$dirname
fi
LD_LIBRARY_PATH=$dirname:$LD_LIBRARY_PATH
export LD_LIBRARY_PATH
ldd $dirname/$appname

export QT_PLUGIN_PATH=$dirname/plugins
$dirname/$appname "$@"

echo $dirname
echo $appname
